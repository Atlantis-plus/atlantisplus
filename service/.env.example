# Supabase (Settings → API)
SUPABASE_URL=https://mhdpokigbprnnwmsgzuy.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJ...   # Settings → API → service_role (secret)
SUPABASE_ANON_KEY=eyJ...           # Settings → API → anon (publishable)
SUPABASE_JWT_SECRET=your-jwt-secret # Settings → API → JWT Settings → JWT Secret

# OpenAI (platform.openai.com → API keys)
OPENAI_API_KEY=sk-...

# Anthropic Claude (console.anthropic.com → API keys)
# Optional: enables /chat/claude endpoint with Claude agent
ANTHROPIC_API_KEY=sk-ant-...

# Telegram (@BotFather → /mybots → API Token)
TELEGRAM_BOT_TOKEN=123456:ABC-DEF...
TELEGRAM_WEBHOOK_SECRET=random-secret-string-here  # Optional: for webhook verification
MINI_APP_URL=https://atlantisplus.pages.dev  # Cloudflare Pages URL for Mini App

# Environment
ENVIRONMENT=development

# People Data Labs (optional, for external enrichment)
# Get API key at: https://www.peopledatalabs.com/
PDL_API_KEY=

# Test mode (for E2E testing - NEVER enable in production)
TEST_MODE_ENABLED=false
TEST_AUTH_SECRET=

# Shared database mode (search across ALL users' data)
# WARNING: For development/demo only! Bypasses owner isolation.
SHARED_DATABASE_MODE=false
